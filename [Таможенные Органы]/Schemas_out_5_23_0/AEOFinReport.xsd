	<xs:simpleType name="GoodsNomenclatuerType">
		<xs:annotation>
			<xs:documentation>Код товара по ТН ВЭД ЕАЭС. от 2 до 10 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CategoryCust:CodeCategoriesCust">
			<xs:pattern value="\d{2,10}"/>
		</xs:restriction>
