      <xs:documentation>Результат поиска операций по карнетам АТА</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="ATA_FindOperationResponseType">
    <xs:annotation>
      <xs:documentation>Результат поиска операций по карнетам АТА</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
