  <xs:annotation>
    <xs:documentation>Уведомление о наличии фактов нарушения сроков временного ввоза</xs:documentation>
  </xs:annotation>
  <xs:element name="ATA_OffenseNotif" type="ata_on:ATA_OffenseNotifType">
