<?xml version="1.0" encoding="UTF-8"?>
<!-- Решение комиссии таможенного союза Евразийского экономического сообщества от 20.05.2010 г. № 260 "О формах таможенных документов". 
- Акт таможенного досмота (таможенного осмотра).
Приказ ФТС России от 31 января 2013 г. N 179 "Об утверждении Инструкции о порядке заполнения, регистрации, хранения, учета актов таможенного досмотра (таможенного осмотра) товаров, пересылаемых в международных почтовых отправлениях".
- Акт таможенного досмотра (таможенного осмотра) товаров, пересылаемых в международных почтовых отправлениях. -->
<xs:schema xmlns:RUScat_ru="urn:customs.ru:RUSCommonAggregateTypes:5.22.0" xmlns:RUSclt_ru="urn:customs.ru:RUSCommonLeafTypes:5.21.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:CategoryCust="urn:customs.ru:Categories:3.0.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.22.0" xmlns:cltESAD_ru="urn:customs.ru:RUCommonLeafTypes:5.4.0" xmlns:catESAD_ru="urn:customs.ru:RUCommonAggregateTypes:5.23.0" xmlns:cltESAD_cu="urn:customs.ru:CUESADCommonLeafTypes:5.17.0" xmlns:catESAD_cu="urn:customs.ru:CUESADCommonAggregateTypesCust:5.22.0" xmlns:ain="urn:customs.ru:Information:CustomsDocuments:ActInspection:5.23.0" targetNamespace="urn:customs.ru:Information:CustomsDocuments:ActInspection:5.23.0" elementFormDefault="qualified" version="5.23.0.0">
  <xs:import namespace="urn:customs.ru:RUSCommonLeafTypes:5.21.0" schemaLocation="RUSCommonLeafTypes.xsd" />
  <xs:import namespace="urn:customs.ru:RUSCommonAggregateTypes:5.22.0" schemaLocation="RUSCommonAggregateTypes.xsd" />
  <xs:import namespace="urn:customs.ru:Categories:3.0.0" schemaLocation="CategoriesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.22.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:RUCommonLeafTypes:5.4.0" schemaLocation="RUESADCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:RUCommonAggregateTypes:5.23.0" schemaLocation="RUESADCommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CUESADCommonLeafTypes:5.17.0" schemaLocation="CUESADCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.22.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Акт таможенного досмотра (таможенного осмотра).</xs:documentation>
  </xs:annotation>
  <xs:element name="ActInspection" type="ain:ActInspectionType">
    <xs:annotation>
      <xs:documentation>Акт таможенного досмотра (таможенного осмотра).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="ActInspectionType">
    <xs:annotation>
      <xs:documentation>Акт таможенного досмотра (таможенного осмотра)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="SendDate" type="clt_ru:DateCustType">
            <xs:annotation>
              <xs:documentation>Дата направления</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActNumber" type="cat_ru:GTDID6Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Номер акта таможенного досмотра (осмотра)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActType" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
              <xs:documentation>Типа акта: "0" - таможенный досмотр, "1" - таможенный осмотр</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MPOSign" type="clt_ru:IndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Признак акта таможенного досмотра (таможенного осмотра) товаров, пересылаемых в МПО</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Direction" type="clt_ru:LetterIndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Направление перемещения: 1 - вывоз, 2 - ввоз</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ShortFormSign" type="clt_ru:IndicatorType">
            <xs:annotation>
              <xs:documentation>Признак составления АТДО в сокращенной форме: 1 - да; 0 - нет</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BeginInspectionDate" type="clt_ru:DateTimeCustType">
            <xs:annotation>
              <xs:documentation>Дата и время начала таможенного досмотра (осмотра)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndInspectionDate" type="clt_ru:DateTimeCustType">
            <xs:annotation>
              <xs:documentation>Дата и время окончания таможенного досмотра (осмотра)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberOfPlaces" type="clt_ru:Count8Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Число мест в партии товаров</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BruttoWeightDocs" type="clt_ru:QuantityBasisType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Вес брутто товаров по документам</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NewCustomsModeCode" type="cltESAD_cu:CustomsModeCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код новой таможенной процедуры - для случая проведения досмотра при изменении процедуры на иную без перемещения товаров через таможенную границу РФ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CustomsMark" type="clt_ru:FreeText250Type" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Служебные отметки </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActKind" type="clt_ru:LetterIndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Признак досмотра: 1 - досмотр всех товаров, заявленных в ДТ, 2 - досмотр части товаров, заявленных в ДТ, 3 - досмотр товаров, заявленных в нескольких ДТ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ObjectType" type="clt_ru:DigitalIndicatorType">
            <xs:annotation>
              <xs:documentation>Тип объекта: 1-товар; 2-транспортное средство</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InspectionFinal" type="clt_ru:IndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Признак того, что досмотр был закончен: 1 - закончен, 0 - не закончен</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NotifInspectionReqGoods" type="clt_ru:DateTimeCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата и время направления уведомления о проведении таможенного досмотра </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndComposeDate" type="clt_ru:DateTimeCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата и время завершения составления АТДО</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FlagDeclarantAbsence" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
              <xs:documentation>Признак отсутствия декларанта: 0 - декларант отсутствует; 1- декларант присутствует</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReasonDeclarantAbsence" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Причины отсутствия декларанта, иных лиц, обладающих полномочиями в отношении товаров, и их представителей  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FlagIdentBreak" type="clt_ru:LetterIndicatorType">
            <xs:annotation>
              <xs:documentation>Признак  наличия нарушенных средств идентификации: 0 - не нарушены; 1- обнаружены нарушенные средства идентификации</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PackingMonitoringFlag" type="clt_ru:LetterIndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Признак упаковки грузовых мест в присутствии наблюдателей: 0 - без наблюдателей; 1 - в присутствии наблюдателей</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InspectionParticipant" type="ain:InspectionParticipantType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Лицо, присутствующее при досмотре (осмотре)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignee" type="ain:ConsigneeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Получатель товаров</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Transporter" type="ain:TransporterType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Перевозчик</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DTNum" type="ain:CustDocNumberType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Номер таможенного документа, в котором заявлены сведения о досматриваемых товарах</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AccDocs" type="ain:AccDocsType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Документы, по которым перевозятся товары (ДТ, книжка МДП, транспортные (перевозочные) документы, коммерческие документы, таможенные документы)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InspectionResults" type="ain:InspectionResultsType">
            <xs:annotation>
              <xs:documentation>Результаты таможенного досмотра (осмотра)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TextAct" type="ain:TextActType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Текст Акта</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Seize" type="ain:SeizeType">
            <xs:annotation>
              <xs:documentation>Изъятие</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AMTInspResults" type="ain:AMTInspResultsType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Результаты досмотра седельного тягача (товарная позиция 8701 по ТН ВЭД ЕАЭС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BusInspResults" type="ain:BusInspResultsType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Результаты досмотра автобуса</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TrailerInspResults" type="ain:TrailerInspResultsType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Результаты досмотра прицепа, полуприцепа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TruckInspResults" type="ain:TruckInspResultsType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Результаты досмотра грузового автомобиля</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MotoInspResults" type="ain:MotoInspResultsType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Результаты досмотра мотоцикла (мопеда)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Customs" type="cat_ru:CustomsType">
            <xs:annotation>
              <xs:documentation>Таможенный орган</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignor" type="ain:ConsignorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Отправитель товаров</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InspectionPlace" type="ain:InspectionPlaceType">
            <xs:annotation>
              <xs:documentation>Место проведения таможенного досмотра (осмотра)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InstructionNumber" type="ain:InspInstructionType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о поручении на досмотр</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ATDCustomsPerson" type="catESAD_ru:ATDCustomPersonType" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Должностное лицо  ТО, присутствовавшее при досмотре</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ATD_MPOCustomsPerson" type="cat_ru:CustomsPersonType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Должностное лицо ТО, направившее второй экземпляр акта с МПО</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AutoInspResults" type="ain:CarInformationType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Результаты досмотра автомобиля (товарная позиция 8702, 8703 по ТН ВЭД ЕАЭС)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InspTransport" type="ain:InspTransportType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о транспортных  средствах</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UseTMCCSign" type="clt_ru:IndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Признак применения технические средства таможенного контроля: 0 - нет, 1 - да</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UseTMCC" type="ain:UseTMCCType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Применяемые технические средства таможенного контроля</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="VOIActNumber" type="cat_ru:GTDIDType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Регистрационный номер акта осмотра с ИДК</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AbnormalNum" type="clt_ru:Text50Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Номер НШС</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1006033E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы.</xs:documentation>
  </xs:annotation>
  <xs:complexType name="ExpressWayBillType">
    <xs:annotation>
      <xs:documentation>Накладная реестра экспресс-грузов</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ObjectOrdinal" type="clt_ru:Count3Type">
        <xs:annotation>
          <xs:documentation>Порядковый номер индивидуальной накладной</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HouseWaybill" type="clt_ru:DocumentNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер индивидуальной накладной</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WayBillID" type="clt_ru:DocumentIDType">
        <xs:annotation>
          <xs:documentation>Уникальный идентификатор накладной</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MPOIdType">
    <xs:annotation>
      <xs:documentation>Сведения об МПО</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MPO_ID" type="clt_ru:DocumentIDType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Уникальный номер записи</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MPO_Number" type="clt_ru:Code13Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Уникальный номер МПО</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecNum" type="clt_ru:Count4Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер МПО в реестре</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="GTINIdentifierType">
    <xs:annotation>
      <xs:documentation>Идентификатор Global Trade Item Number (GTIN)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="clt_ru:Code14Type">
      <xs:pattern value="[0-9]{14}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="RFID_TIDType">
    <xs:annotation>
      <xs:documentation>Идентификатор чипа радиочастотной метки (RFID) TID</xs:documentation>
    </xs:annotation>
    <xs:restriction base="CategoryCust:CodeCategoriesCust">
      <xs:pattern value="[A-F0-9]{24}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AccDocsType">
    <xs:annotation>
      <xs:documentation>Документы, по которым перевозятся товары (ДТ, книжка МДП, транспортные (перевозочные) документы, коммерческие документы, таможенные документы)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:DocumentBaseType">
        <xs:sequence>
          <xs:element name="PresentedDocumentModeCode" type="clt_ru:CustomsDocumentCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код вида документа. Заполняется в соответствии с классификатором видов документов, используемых при заявлении сведений в графе 44</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AKArticlesType">
    <xs:annotation>
      <xs:documentation>Cтатья Кодекса РФ об административных правонарушениях </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ArticleNumber" type="clt_ru:Text5Type">
        <xs:annotation>
          <xs:documentation>Номер статьи</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PartNumber" type="clt_ru:Text4Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Номер части статьи</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AMTDopDeviceType">
    <xs:annotation>
      <xs:documentation>Дополнительное оборудование в тягаче</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CabCowl" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Надкабинный обтекатель: "0" - нет, "1" - нерегулируемый по высоте, "2" - регулируемый по высоте</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ABS" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Антиблокировочная система: "0" - нет, "1" - есть</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Conditioner" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Кондиционер: "0" - нет, "1" - кондиционер, "2" - климат контроль</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Signalization" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Сигнализация: "0" - нет, "1" - есть</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="VideotapeRecorder" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Видеомагнитофон: "0" - нет, "1" - есть</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AudioSystem" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Аудиосистема: "0" - нет, "1" - есть</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TV" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Телевизор: "0" - нет, "1" - есть</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Telefon" type="clt_ru:Quantity2Type">
        <xs:annotation>
          <xs:documentation>Количество телефонов: "0" - если нет</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Fax" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Факс: "0" - нет, "1" - есть</xs:documentation>
