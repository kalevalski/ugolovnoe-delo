            <xs:annotation>
              <xs:documentation>Сведения о таможенном органе</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Resident" type="apertg_stz:ResidentType">
            <xs:annotation>
              <xs:documentation>Сведения о резиденте СТЗ</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AppProlongRegNum" type="apertg_stz:AppProlongRegNumType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Регистрационный номер Заявления о продлении срока обратного ввоза</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AppRegNumber" type="apertg_stz:AppRegNumType">
            <xs:annotation>
              <xs:documentation>Регистрационный номер Заявления на вывоз с участка резидента Арктической зоны</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Reason" type="clt_ru:FreeText250Type" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Причины продления срока обратного ввоза товаров</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ConfirmDoc" type="catProc_ru:DocExtendedType" minOccurs="0" maxOccurs="unbounded">
