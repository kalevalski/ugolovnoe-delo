<?xml version="1.0" encoding="UTF-8"?>
<!-- Постановление Правительства РФ от 21.12.2020 № 2200 "Об утверждении Правил перевозок грузов автомобильным транспортом и о внесении изменений в пункт 2.1.1 Правил дорожного движения Российской Федерации"
Приложение №4 (в редакции постановления Правительства РФ от 30.11.2021 № 2116)  -->
<xs:schema xmlns:RUScat_ru="urn:customs.ru:RUSCommonAggregateTypes:5.22.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.22.0" xmlns:cltTrans_ru="urn:customs.ru:Information:TransportDocuments:TransportCommonLeafTypesCust:5.14.3" xmlns:cltESAD_cu="urn:customs.ru:CUESADCommonLeafTypes:5.17.0" xmlns:catESAD_cu="urn:customs.ru:CUESADCommonAggregateTypesCust:5.22.0" xmlns:atb="urn:customs.ru:Information:TransportDocuments:AutoBill:5.22.0" xmlns:catComFin_ru="urn:customs.ru:Information:CommercialFinanceDocuments:CommercialFinanceCommonAgregateTypesCust:5.22.0" targetNamespace="urn:customs.ru:Information:TransportDocuments:AutoBill:5.22.0" elementFormDefault="qualified" version="5.22.0.0">
  <xs:import namespace="urn:customs.ru:RUSCommonAggregateTypes:5.22.0" schemaLocation="RUSCommonAggregateTypes.xsd" />
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.22.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CUESADCommonLeafTypes:5.17.0" schemaLocation="CUESADCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.22.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:TransportDocuments:TransportCommonLeafTypesCust:5.14.3" schemaLocation="TransportCommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:CommercialFinanceDocuments:CommercialFinanceCommonAgregateTypesCust:5.22.0" schemaLocation="CommercialFinanceCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Автомобильная транспортная накладная</xs:documentation>
  </xs:annotation>
  <xs:element name="AutoBill" type="atb:AutoBillType">
    <xs:annotation>
      <xs:documentation>Автомобильная транспортная накладная</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AutoBillType">
    <xs:annotation>
      <xs:documentation>Автомобильная транспортная накладная (тип)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="NumberTN" type="clt_ru:DocumentNumberType">
            <xs:annotation>
              <xs:documentation>Номер ТН</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DateTN" type="clt_ru:DateCustType">
            <xs:annotation>
              <xs:documentation>Дата ТН</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberCopies" type="clt_ru:Count3Type">
            <xs:annotation>
              <xs:documentation>Номер экземпляра</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Order" type="atb:OrderType">
            <xs:annotation>
              <xs:documentation>Заказ (заявка) и информация о принятии заказа к исполнению</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DestinationOfficeIdentifier" type="clt_ru:CustomsCodeType">
            <xs:annotation>
              <xs:documentation>Код таможенного органа назначения, в регионе деятельности которого расположено место доставки</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignor" type="cat_ru:OrganizationType">
            <xs:annotation>
              <xs:documentation>Сведения о грузоотправителе (раздел 1)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Forwarder" type="clt_ru:IndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Грузоотправитель является экспедитором: 0 - нет; 1 - да</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CalculationDoc" type="cat_ru:DocumentBaseType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Реквизиты документа, определяющего основания осуществления расчетов по договору перевозки иным лицом, отличным от грузоотправителя</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Customer" type="cat_ru:OrganizationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о заказчике услуг по организации перевозки груза (раздел 1а)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TransportationContract" type="cat_ru:DocumentBaseType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Реквизиты договора на выполнение услуг по организации перевозки груза</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignee" type="cat_ru:OrganizationType">
            <xs:annotation>
              <xs:documentation>Сведения о грузополучателе (раздел 2)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="DeliveryPlace" type="clt_ru:NameType">
              <xs:annotation>
                <xs:documentation>Место доставки</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeliveryAddress" type="cat_ru:AddressType">
              <xs:annotation>
                <xs:documentation>Адрес доставки</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element name="Cargo" type="atb:CargoType" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Наименование груза (раздел 3)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocumentTN" type="atb:DocumentType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Документы, приложенные к ТН (раздел 4)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocumentCargo" type="atb:DocumentType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Документы, приложенные к  грузу (раздел 4)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocumentShipment" type="atb:DocumentType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Реквизиты документов, подтверждающих отгрузку товаров, и сопроводительной ведомости (раздел 4)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ConsignorInstruction" type="atb:ConsignorInstructionType">
            <xs:annotation>
              <xs:documentation>Указания грузоотправителя (раздел 5)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LoadCargo" type="atb:LoadCargoType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Прием груза (раздел 8)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UnloadCargo" type="atb:LoadUnloadCargoType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Выдача груза (раздел 10)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Carrier" type="atb:CarrierType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Перевозчик (раздел 6)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RoadTrain" type="clt_ru:IndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Перевозка осуществляется автопоездом: 0 - нет; 1 - да</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Transport" type="atb:TransportType" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Транспортное средство (раздел 7)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CarrierComments" type="atb:CarrierCommentsType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Оговорки и замечания перевозчика</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Readdressing" type="atb:ReaddressingType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Переадресовка (раздел 9)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Calculation" type="atb:CalculationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Стоимость перевозки груза (установленная плата) в рублях (раздел 12)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Notes" type="atb:NoteType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Отметки грузоотправителей, грузополучателей и перевозчиков (раздел 11)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="DocumentModeID" type="clt_ru:DocumentModeIDType" use="required" fixed="1003103E">
          <xs:annotation>
            <xs:documentation>Идентификатор вида документа</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>Локальные прикладные типы.</xs:documentation>
  </xs:annotation>
  <xs:complexType name="DocumentType">
    <xs:annotation>
      <xs:documentation>Приложенные документы</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:DocumentBaseType">
        <xs:sequence>
          <xs:element name="INN" type="clt_ru:INNIDType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>ИНН составителя документа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DocumentModeCode" type="clt_ru:CustomsDocumentCodeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Код вида документа в соответствии с классификатором видов документов и сведений, используемых при таможенном декларировании</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExpirationDate" type="clt_ru:DateCustType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Дата окончания действия документа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReleaseOrganization" type="clt_ru:FreeText250Type" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Организация, выдавшая документ</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TransportType">
    <xs:annotation>
      <xs:documentation>Сведения об ТС</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TypeRoadTrain" type="clt_ru:Numeric1Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Часть автопоезда: 1 - тягач; 2 - прицеп; 3 - полуприцеп</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TypeOwnership" type="clt_ru:Numeric1Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Тип владения транспортным средством перевозчиком: 1 - собственность; 2 - совместная собственность супругов; 3 - аренда; 4 - лизинг; 5 - безвозмездное пользование</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RegistrationNumber" type="clt_ru:TransportMeansIDType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Регистрационный номер ТС</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Kind" type="cltESAD_cu:TransportKindCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Тип ТС</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Mark" type="clt_ru:Text40Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Марка ТС</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Tonnage" type="clt_ru:WeighQuantityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Грузоподъемность (тоннаж)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Capacity" type="clt_ru:Quantity14point2Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Вместимость в куб.м</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BasisOwnership" type="cat_ru:DocumentBaseType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Реквизиты документа(-ов), подтверждающего(-их) основание владения грузовым автомобилем (тягачом, а также прицепом (полуприцепом))</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SpecialPermission" type="cat_ru:DocumentBaseType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Реквизиты специального разрешения</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Validity" type="clt_ru:DateCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Срок действия специального разрешения, </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HeavyOversized" type="clt_ru:IndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Является транспортное средство тяжеловесным и (или) крупногабаритным: 0 - нет; 1 - да</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DangerousCargo" type="clt_ru:IndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Транспортное средство перевозит опасный груз: 0 - нет; 1 - да</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Route" type="atb:RouteType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Установленный маршрут движения</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SealType">
    <xs:annotation>
      <xs:documentation>Информация о пломбах или ЗПУ</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IdentNumber" type="clt_ru:Text50Type">
        <xs:annotation>
          <xs:documentation>Номер наложенного средства идентификации</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdentKind" type="clt_ru:LetterIndicatorType">
        <xs:annotation>
          <xs:documentation>Вид средств идентификации: 1 - пломба, 2 - печать, 3 - иное</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IdentDesc" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Примечание по средству идентификации и его состоянию</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Quantity" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AmountType">
    <xs:annotation>
      <xs:documentation>Сумма и валюта</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CurrencyCode" type="clt_ru:CurrencyA3CodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Трехзначный буквенный код валюты суммы. По классификатору валют</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmount" type="clt_ru:AmountType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Cумма</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LoadCargoType">
    <xs:annotation>
      <xs:documentation>Прием груза</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="atb:LoadUnloadCargoType">
        <xs:sequence>
          <xs:element name="LoadingPointOwner" type="cat_ru:OrganizationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения о владельце объекта инфраструктуры пункта погрузки</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Loading" type="clt_ru:IndicatorType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Погрузка груза в транспортное средство осуществлена: 0 - грузоотправителем, 1 - иным уполномоченным грузоотправителем лицом</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LoadingPerson" type="cat_ru:OrganizationType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Сведения об уполномоченном грузоотправителем лице, осуществившем погрузку груза в транспортное средство</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LoadingPersonDoc" type="cat_ru:DocumentBaseType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Реквизиты документов, подтверждающих полномочия лица</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LoadUnloadCargoType">
    <xs:annotation>
      <xs:documentation>Погрузка/выгрузка груза</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Address" type="cat_ru:AddressType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Адрес места погрузки/выгрузки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DateTime" type="clt_ru:DateTimeCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Дата и время подачи ТС на погрузку/выгрузку</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ActualDateTime" type="clt_ru:DateTimeCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Фактические дата и время прибытия ТС на погрузку/выгрузку</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ActualStateOfPackages" type="atb:PackingInformationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Фактическое состояние груза, упаковки, маркировки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Seal" type="atb:SealType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Фактическое состояние опломбирования</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CargoWeight" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Масса груза в кг</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlacesQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Количество грузовых мест</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PersonSign" type="atb:PersonSignatureType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Подпись грузоотправителя/грузополучателя/водителя</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ActualDepDateTime" type="clt_ru:DateTimeCustType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Фактические дата и время убытия ТС с погрузки/выгрузки</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CargoWeightCalculation" type="clt_ru:Numeric1Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Метод определения массы брутто: 1 - определение разницы между массой транспортного средства после погрузки и перед погрузкой по общей массе; 2 - взвешивание поосно; 3 - расчетная масса груза</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NettoCargoWeight" type="clt_ru:QuantityBasisType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Масса груза нетто в кг</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PackingInformationType">
    <xs:annotation>
      <xs:documentation>Сведения об упаковке и маркировке</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PackageTypeCode" type="clt_ru:LetterIndicatorType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Код наличия упаковки товара: 0 - Без упаковки; 1 - С упаковкой; 2 - Без упаковки в оборудованных емкостях транспортного средства</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CargoState" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Фактическое состояние груза</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PackingMark" type="clt_ru:FreeText250Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Маркировка грузовых мест</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PackingInfo" type="catESAD_cu:PackagePalleteInformationType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Информация об упаковке</xs:documentation>
        </xs:annotation>
