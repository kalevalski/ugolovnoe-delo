          <xs:documentation>Сведения о покупателе</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LoadingLocationDetails" type="catpi_ru:PI_EEC_CargoLocationDetailsType" minOccurs="0">
