          <xs:documentation>Сведения о планируемом месте временного хранения товаров</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GoodsItem" type="catpi_ru:PI_EEC_GoodsItemType" maxOccurs="unbounded">
