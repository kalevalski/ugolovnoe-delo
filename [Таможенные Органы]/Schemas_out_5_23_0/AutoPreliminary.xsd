        </xs:annotation>
      </xs:element>
      <xs:element name="ConsigneeDetails" type="catpi_ru:PI_EEC_SubjectDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Сведения о получателе товаров</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SellerDetails" type="catpi_ru:PI_EEC_SubjectDetailsType" minOccurs="0">
