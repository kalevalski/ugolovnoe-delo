	<xs:import namespace="urn:customs.ru:CUESADCommonAggregateTypesCust:5.22.0" schemaLocation="CUESADCommonAggregateTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation> ДТС. Общие сложные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AdditionalDataType">
		<xs:annotation>
			<xs:documentation>Дополнительная информация, для которой не предусмотрено специальных элементов</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CVDAdditionalInformationKindCode" type="clt_ru:LetterIndicatorType" minOccurs="0">
