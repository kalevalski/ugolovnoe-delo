<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.22.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" targetNamespace="urn:customs.ru:CommonAggregateTypes:5.22.0" elementFormDefault="qualified" version="5.22.0.0">
	<xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd"/>
	<xs:annotation>
		<xs:documentation>Общие сложные прикладные типы</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>Адрес организации / физического лица</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostalCode" type="clt_ru:PostalCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Почтовый индекс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Буквенный код страны в соответствии с классификатором стран мира</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CounryName" type="clt_ru:CountryNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Краткое название страны в соответствии с классификатором стран мира</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Region" type="clt_ru:RegionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Область (регион, штат, провинция и т.п.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="clt_ru:CityNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Населенный пункт</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StreetHouse" type="clt_ru:StreetHouseType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Улица, номер дома, номер офиса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TerritoryCode" type="clt_ru:TerritoryCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код административно-территориальной единицы в соответствии с ГК СОАТЕ (для Кыргызской Республики)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AuthoritesDocumentType">
		<xs:annotation>
			<xs:documentation>Сведения о документе, удостоверяющем полномочия</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:DocumentBaseType">
				<xs:sequence>
					<xs:element name="ComplationAuthorityDate" type="clt_ru:DateCustType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Дата окончания полномочий</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AutomobileBaseType">
		<xs:annotation>
			<xs:documentation>Данные об автомобилях. Базовый класс для 31й гр. и  Карточки транспортного средства</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Model" type="clt_ru:TransportNameType">
				<xs:annotation>
					<xs:documentation>Марка, модель</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Mark" type="clt_ru:MarkCarNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование марки транспортного средства в соответствии с классификатором марок дорожных транспортных средств</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MarkCode" type="clt_ru:MarkCarCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код марки транспортного средства в соответствии с классификатором марок дорожных транспортных средств</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OfftakeYear" type="clt_ru:DateYearCustType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Год изготовления транспортного средства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EngineVolumeQuanity" type="clt_ru:EngineVolumeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Рабочий объем двигателя, куб. см</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VINID" type="clt_ru:VINIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификационный номер (VIN)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BodyID" type="clt_ru:VINIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификационный номер кузова (прицепа), присвоенный и нанесенный на него организацией-изготовителем</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EngineID" type="clt_ru:EngineIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Присвоенный организацией - изготовителем идентификационный номер двигателя, нанесенный на блоке двигателя. Идентификационный номер может состоять из групп раздельных цифр, из которых последняя группа, состоящая из двух цифр, указывает год выпуска двигателя</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChassisID" type="clt_ru:VINIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификационный номер шасси (рамы), присвоенные и нанесенные на них организацией-изготовителем</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabID" type="clt_ru:VINIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификационный номер кабины</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmergencyDeviceID" type="clt_ru:Text50Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификационный номер устройства или системы вызова экстренных оперативных служб, которыми оборудовано транспортное средство. Для РФ</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BankInformationType">
		<xs:annotation>
			<xs:documentation>Информация о банковских счетах</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BankAccount" type="clt_ru:ForeignAccountType" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Номер банковского счета</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankAccountDescription" type="clt_ru:FreeText250Type" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Описание типа счета: расчетный, валютный</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankName" type="clt_ru:NameType">
				<xs:annotation>
					<xs:documentation>Название банка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BankMFO" type="clt_ru:BankMFOType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>МФО банка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKPOID" type="clt_ru:OKPOIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код банка по ОКПО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BICID" type="clt_ru:BICIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>БИК банка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SWIFTID" type="clt_ru:SWIFTIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SWIFT код банка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CorrAccount" type="clt_ru:ForeignAccountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер корреспондентского счета банка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransitCurrencyAccount" type="clt_ru:ForeignAccountType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Валютный счет организации (транзитный)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialTransitCurrencyAccount" type="clt_ru:ForeignAccountType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Валютный счет организации (специальный транзитный)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BaseDocType">
		<xs:annotation>
			<xs:documentation>Базовый тип для электронного документа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocumentID" type="clt_ru:DocumentIDType">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefDocumentID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор исходного документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="INNSign" type="clt_ru:INNIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ИНН владельца МЧД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MCD_ID" type="clt_ru:DocumentIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Единый регистрационный номер доверенности в формате МЧД</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CUBranchDescriptionType">
		<xs:annotation>
			<xs:documentation>Сведения об обособленном подразделении</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationName" type="clt_ru:ShortNameType">
				<xs:annotation>
					<xs:documentation>Краткое наименование обособленного подразделения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="cat_ru:AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Адрес обособленного подразделения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="RFOrganizationFeatures" type="cat_ru:RFOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Российской Федерации</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RKOrganizationFeatures" type="cat_ru:RKOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Республики Казахстан</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RBOrganizationFeatures" type="cat_ru:RBOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Республики Беларусь</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RAOrganizationFeatures" type="cat_ru:RAOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Республики Армения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="KGOrganizationFeatures" type="cat_ru:KGOrganizationFeaturesType">
					<xs:annotation>
						<xs:documentation>Сведения об организации. Особенности Кыргызской Республики</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CUCustomsType">
		<xs:annotation>
			<xs:documentation>Сведения о таможенном органе</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:CustomsType">
				<xs:sequence>
					<xs:element name="CustomsCountryCode" type="clt_ru:CountryN3CodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Код государства - члена Евразийского экономического союза. Трехзначный цифровой код</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CUOrganizationType">
		<xs:annotation>
			<xs:documentation>Сведения об организации  (используется до версии АФ 5.14.3)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cat_ru:OrganizationBaseType">
				<xs:sequence>
					<xs:element name="Address" type="cat_ru:AddressType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Адрес организации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IdentityCard" type="cat_ru:IdentityCardType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Документ, удостоверяющий личность</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Contact" type="cat_ru:ContactType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Контактная информация</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CUTransportMeansType">
		<xs:annotation>
			<xs:documentation>Транспортные средства на границе / внутри страны</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransportModeCode" type="clt_ru:TransportModeCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код вида транспортного средства по классификатору видов транспорта и транспортировки товаров</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransportNationalityCode" type="clt_ru:CountryA2CodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Буквенный код страны принадлежности (регистрации) транспортных средств по классификатору стран мира. 99-разные, 00-неизвестна</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CargoBaseType">
		<xs:annotation>
			<xs:documentation>Сведения о грузе</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CargoDescription" type="clt_ru:FreeText250Type" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Описание груза</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossWeightQuantity" type="clt_ru:QuantityBasisType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вес товара брутто (кг)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContactType">
		<xs:annotation>
			<xs:documentation>Контактная информация</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Phone" type="clt_ru:PhoneNumberType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Номер телефона</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fax" type="clt_ru:PhoneNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер факса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Telex" type="clt_ru:PhoneNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер телекса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="E_mail" type="clt_ru:Text50Type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Адрес электронной почты</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsPersonType">
		<xs:annotation>
			<xs:documentation>Должностное лицо таможенного органа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PersonName" type="clt_ru:NameType">
				<xs:annotation>
					<xs:documentation>ФИО должностного лица таможенного органа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LNP" type="clt_ru:LNPIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ЛНП должностного лица таможенного органа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomsCode" type="clt_ru:CustomsCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код таможенного органа, указанный на оттиске ЛНП</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsType">
		<xs:annotation>
			<xs:documentation>Код и наименование таможенного органа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code" type="clt_ru:CustomsCodeType">
				<xs:annotation>
					<xs:documentation>Код таможенного органа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OfficeName" type="clt_ru:CustomsOfficeNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование таможенного органа</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeliveryTermsRUType">
		<xs:annotation>
			<xs:documentation>Условие поставки товаров. Для России</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeliveryPlace" type="clt_ru:Text50Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание / Название географического пункта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryTermsStringCode" type="clt_ru:IncotermsDeliveryStringCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код условий поставки в соответствии с классификатором условий поставки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeliveryTermsType">
		<xs:annotation>
			<xs:documentation>Условие поставки товаров</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeliveryPlace" type="clt_ru:Text50Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание / Название географического пункта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryTermsStringCode" type="clt_ru:IncotermsDeliveryStringCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код условий поставки в соответствии с классификатором условий поставки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryTermsRBCode" type="clt_ru:Code2Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код вида поставки товаров в соответствии с классификатором видов поставок товаров, подлежащих учету при осуществлении экспортных операций, применяемым в Республике Беларусь</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransferPlace" type="clt_ru:FreeText250Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Место передачи товара в соответствии с условиями договора (контракта)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DimensionsType">
		<xs:annotation>
			<xs:documentation>Габариты</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DimensionsCode" type="clt_ru:CodeVariable3Type">
				<xs:annotation>
					<xs:documentation>Код единицы измерения </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Lenght" type="clt_ru:Quantity5Type">
				<xs:annotation>
